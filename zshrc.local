# recommended by brew doctor
export PATH="/Users/mcritchlow/bin:/usr/local/bin:/usr/local/sbin:$PATH"
# node/npm
export PATH="$PATH:/usr/local/lib/node_modules"
source $(brew --prefix nvm)/nvm.sh
